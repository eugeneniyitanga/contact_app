<h1> Registered Contacts </h1>
 
<table class="table table-hover">
  <tr>
      <th>First Name</th>
      <th>Middle Name</th>
      <th> Last Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Bio</th>
  </tr>
  <br>
    <% @contacts.each do |contact| %>
    <tr>
    <td><%= contact.first_name %></td>
    <td> <%= contact.middle_name%></td>
    <td><%= contact.last_name %></td>
    <td><%= contact.email %></td>
    <td><%= contact.phone_number %></td>
    <td><%= contact.bio %></td>
  </tr>
  <% end %>
</table>